:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: 'Muli', sans-serif;
	--theme-font-family-heading: 'Muli', sans-serif;
	--theme-font-color-base: var(--color-primary-900);
	--theme-font-color-dark: var(--color-tertiary-500);
	--theme-rounded-base: 9999px;
	--theme-rounded-container: 12px;
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: var(--color-tertiary-200);
	--on-secondary: 0 0 0;
	--on-tertiary: 0 0 0;
	--on-success: var(--color-secondary-400);
	--on-warning: var(--color-surface-900);
	--on-error: var(--color-secondary-400);
	--on-surface: var(--color-tertiary-500);
	/* =~= Theme Colors  =~= */
	/* primary | #144e6c */
	--color-primary-50: 220 228 233; /* ⬅ #dce4e9 */
	--color-primary-100: 208 220 226; /* ⬅ #d0dce2 */
	--color-primary-200: 196 211 218; /* ⬅ #c4d3da */
	--color-primary-300: 161 184 196; /* ⬅ #a1b8c4 */
	--color-primary-400: 91 131 152; /* ⬅ #5b8398 */
	--color-primary-500: 20 78 108; /* ⬅ #144e6c */
	--color-primary-600: 18 70 97; /* ⬅ #124661 */
	--color-primary-700: 15 59 81; /* ⬅ #0f3b51 */
	--color-primary-800: 12 47 65; /* ⬅ #0c2f41 */
	--color-primary-900: 10 38 53; /* ⬅ #0a2635 */
	/* secondary | #d1cbb8 */
